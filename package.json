{
  "name": "kio-ng2-component-routing",
  "version": "0.1.2",
  "description": "kio module for ng2 component matching",
  "main": "./src/module",
  "module": "./src/module",
  "typings": "./src/module.ts",
  "scripts": {
    "prebuild": "rm -rf ./release",
    "transpile": "npm run ngModule:build",
    "ngModule:build": "./scripts/ngModule.sh build",
    "copy:dev": "./scripts/sync_to.sh ../../digit-wmr",
    "build": "npm run ngModule:build",
    "build:watch": "./scripts/watch.sh build",
    "build:release": "NODE_ENV=production npm run build && npm run copy:dev",
    "build:release:watch": "./scripts/watch.sh build:release",
    "preversion": "npm run build",
    "dev:watch": "nodemon -w ./src -e ts --exec \"npm run build\"",
    "test": "ts-mocha ./src/**/*.spec.ts",
    "test:watch": "./scripts/watch.sh test",
    "test:assertion": "ts-mocha ./src/query/assertion/*.spec.ts",
    "test:assertion:watch": "./scripts/watch.sh test:assertion",
    "test:fixture": "ts-mocha ./src/fixture/fixture.spec.ts",
    "test:fixture:watch": "./scripts/watch.sh test:fixture"
  },
  "keywords": [],
  "author": "Matthias Buchholz <matthias@afkm.io>",
  "license": "ISC",
  "peerDependencies": {
    "kio-ng2-data": "~0.0.1",
    "kio-ng2-ctn": "~0.0.7",
    "kio-ng2-i18n": "~0.0.2",
    "kio-ng2-scrolling": "~0.0.4",
    "@angular/common": "^4.2.3",
    "@angular/compiler": "^4.2.3",
    "@angular/core": "^4.2.3",
    "@angular/http": "^4.2.3",
    "@angular/platform-browser": "^4.2.3",
    "@angular/platform-browser-dynamic": "^4.2.3",
    "cuid": "^1.3.8",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/common": "^4.2.3",
    "@angular/compiler": "^4.2.3",
    "@angular/compiler-cli": "^4.2.3",
    "@angular/core": "^4.2.3",
    "@angular/platform-browser": "^4.2.3",
    "@angular/platform-browser-dynamic": "^4.2.3",
    "@types/chai": "^4.0.4",
    "@types/cuid": "0.0.30",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.12",
    "ceylon": "^0.9.1",
    "chai": "^4.1.2",
    "cuid": "^1.3.8",
    "kio-ng2-ctn": "github:afkm/kio-ng2-ctn#v0.0.7",
    "kio-ng2-data": "github:afkm/kio-ng2-data#v0.0.1",
    "kio-ng2-i18n": "github:afkm/kio-ng2-i18n#v0.0.2",
    "kio-ng2-scrolling": "github:afkm/kio-ng2-scrolling#v0.0.4",
    "mocha": "^3.3.0",
    "nodemon": "^1.11.0",
    "rxjs": "^5.4.2",
    "ts-mocha": "^1.0.3",
    "tslint": "^5.0.0",
    "typescript": "2.3.4",
    "zone.js": "^0.8.12"
  },
  "dependencies": {
    "lodash.zip": "^4.2.0",
    "lodash.zipobject": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afkm/kio-ng2-component-routing.git"
  },
  "bugs": {
