{
  "name": "kio-ng2-component-routing",
  "version": "0.0.6",
  "description": "kio module for ng2 component matching",
  "main": "release/index.js",
  "module": "src/index.ts",
  "typings": "release/index.d.ts",
  "scripts": {
    "copy:release": "./scripts/copy_release.sh",
    "prebuild": "rm -rf ./release",
    "build": "NODE_ENV=production tsc",
    "build:watch": "./scripts/watch.sh build",
    "build:release": "NODE_ENV=production tsc",
    "postbuild:release": "npm run copy:release",
    "build:release:watch": "./scripts/watch.sh build:release",
    "preversion": "npm run build",
    "dev:watch": "nodemon -w ./src -e ts --exec \"npm run build\"",
    "prebuild:release": "rm -rf ./release",
    "test": "ts-mocha ./src/**/*.spec.ts",
    "test:watch": "./scripts/watch.sh test",
    "test:fixture": "ts-mocha ./src/fixture/fixture.spec.ts",
    "test:fixture:watch": "./scripts/watch.sh test:fixture",
    "pretypedoc": "rm -rf ./doc",
    "typedoc": "typedoc --target es6 --name kio-ng2-component-routing --exclude *.spec.ts --out doc ./src",
    "typedoc:watch": "./scripts/watch.sh typedoc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cuid": "0.0.30",
    "@types/lodash": "^4.14.62",
    "@types/node": "^7.0.12",
    "cuid": "^1.3.8",
    "kio-ng2": "github:afkmatze/kio-ng2#v0.0.1",
    "lodash": "^4.17.4",
    "lodash.filter": "^4.6.0",
    "lodash.find": "^4.6.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "ceylon": "^0.9.1",
    "mocha": "^3.3.0",
    "nodemon": "^1.11.0",
    "ts-mocha": "^1.0.3",
    "tslint": "^5.0.0",
    "typedoc": "^0.7.1",
    "typedoc-dash-theme": "^1.1.0",
    "typedoc-markdown-theme": "0.0.4",
    "typedoc-plugin-external-module-name": "^1.0.9",
    "typedoc-plugin-internal-external": "^1.0.9",
    "typescript": "^2.2.2"
  }
}
